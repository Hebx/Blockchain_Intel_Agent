# AI SDK Configuration
# Get your API key from: https://vercel.com/docs/ai-sdk/introduction
VERCEL_AI_KEY=your_vercel_ai_key
OPENAI_API_KEY=your_openai_api_key
ANTHROPIC_API_KEY=your_anthropic_api_key

# Blockscout MCP Server
BLOCKSCOUT_MCP_URL=https://mcp.blockscout.com
DEFAULT_CHAIN=ethereum

# Cache & Rate Limiting
REDIS_URL=redis://localhost:6379
CACHE_TTL_LATEST_BLOCK=30
CACHE_TTL_TOKEN_HOLDERS=300
CACHE_TTL_ACCOUNT_SUMMARY=60

# Node Environment
NODE_ENV=development

# Application
NEXT_PUBLIC_APP_URL=http://localhost:3000
PORT=3000
